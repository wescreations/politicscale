	<div class="row">
		<div class="col-sm-8">
			<div class="form-group"  style="width: 100%;">
				<label>Name</label>
				<%= f.text_field :name, :class=> 'form-control'  %>
			</div>
		</div>
		<div class="col-sm-4">
			<div class="form-group">
				<label>Score</label>
				<%= f.number_field :score, :class=> 'form-control'  %>
			</div>
		</div>
	</div>
	<div class="row">
		<div class="col-sm-4">
				<label>Age</label>
				<%= f.text_field :age, :class=> 'form-control'  %>
		</div>
	</div>
	<div class="row">
		<div class="col-sm-4">
			<div class="form-group">
				<label>Party</label>
				<%= f.select :party, [" ", "Democrat","Republican","Independent"], {}, { class: "form-control" } %>
			</div>
		</div>
		<div class="col-sm-6">
			<div class="form-group">
				<label>Home State</label>
